            "user_data": "#!/bin/bash\r\nset -e\r\n\r\n# 1. Pass Stage Parameter \u0026 Load Config\r\necho \"Starting deployment for stage: dev\"\r\n\r\nif [ \"$stage\" == \"dev\" ]; then\r\n  echo \"Loading dev configuration...\"\r\nelif [ \"$stage\" == \"prod\" ]; then\r\n  echo \"Loading prod configuration...\"\r\nelse\r\n  echo \"Unknown stage 'dev'. Using default behavior.\"\r\nfi\r\n\r\n# 2. Install Dependencies\r\necho \"Updating packages and installing Java 21, Maven, Git...\"\r\nsudo apt-get update -y\r\nsudo apt-get install -y openjdk-21-jdk maven git\r\n\r\n# 3. Clone Repository and Build Application\r\necho \"Cloning repository from GitHub...\"\r\ngit clone https://github.com/Trainings-TechEazy/test-repo-for-devops.git\r\ncd test-repo-for-devops\r\nmvn clean package\r\n\r\n# 4. Run the Application in the Background\r\necho \"Starting the application...\"\r\nnohup java -jar target/techeazy-devops-0.0.1-SNAPSHOT.jar \u003e /dev/null 2\u003e\u00261 \u0026\r\n\r\n# 5. Test if App is Reachable\r\necho \"Checking if the application is running on port 9090...\"\r\nATTEMPTS=0\r\nMAX_ATTEMPTS=12\r\nwhile [ $ATTEMPTS -lt $MAX_ATTEMPTS ]; do\r\n  HTTP_STATUS=$(curl -s -o /dev/null -w \"%{http_code}\" http://localhost:9090)\r\n  if [ \"$HTTP_STATUS\" == \"200\" ]; then\r\n    echo \"Application is up and running!\"\r\n    break\r\n  else\r\n    echo \"App not yet reachable (status: $HTTP_STATUS). Retrying in 5 seconds...\"\r\n    sleep 5\r\n    ATTEMPTS=$((ATTEMPTS + 1))\r\n  fi\r\ndone\r\n\r\nif [ $ATTEMPTS -eq $MAX_ATTEMPTS ]; then\r\n  echo \"ERROR: Application did not become reachable within the timeout period.\"\r\nfi\r\n\r\n\r\n# 6. Schedule Instance Shutdown for Cost Savings\r\necho \"Scheduling instance to shut down in 30 minutes...\"\r\nsudo shutdown -h +30\r\n\r\necho \"Deployment complete!\"\r\n",
